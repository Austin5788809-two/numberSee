{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang++.exe 生成活动文件",
            "command": "F:/CODE/LLVM/bin/clang++.exe",
            "args": [
                "-fcolor-diagnostics",
                "-I",
                "F:/CODE/cpp/lib/include",
                "-g",
                "net.cpp",
                "${file}",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}